<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Index</title>
    <link rel="stylesheet" href="../assets/css/site.css">
  </head>
  <body>
    <div id="content">
      <h1>Index</h1>
      <% pages.forEach(function(page) { %>
        <%= dateformat(page.meta.date, 'mm/dd/yyyy') %> <a href="<%= page.href %>"><%= page.meta.title %></a><br/>
      <% }) %>
    </div>
  </body>
</html>
